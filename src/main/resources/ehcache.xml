<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
    <diskStore path="ehcache/AdProducer"/>
    <defaultCache
        maxElementsInMemory="100"
        eternal="false"
        overflowToDisk="true"
        memoryStoreEvictionPolicy="LFU" />
    <cache name="${conf.cache1}"
           maxElementsInMemory="100"
           eternal="false"
           overflowToDisk="true"
           diskPersistent="true"
           memoryStoreEvictionPolicy="LFU" />
    <cache name="${conf.cache2}"
           maxElementsInMemory="100"
           eternal="false"
           overflowToDisk="true"
           diskPersistent="true"
           memoryStoreEvictionPolicy="LFU" />     
</ehcache>